{
  "name": "backbone-base",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com:fs/backbone-base.git"
  },
  "authors": [
    "Marat Fakhreev <marat.fakhreev@flatstack.com>",
    "Anton Gudkov <anton.gudkov@flatstack.com>",
    "Timur Vafin <timur.vafin@flatstack.com>",
    "Ildus Sadykov <ildus.sadykov@flatstack.com>"
  ],
  "devDependencies": {
    "gulp": "~3.8.8",
    "gulp-plumber": "~0.6.6",
    "gulp-install": "~0.2.0",
    "gulp-ignore": "~1.2.1",
    "gulp-jade": "~0.8.0",
    "gulp-stylus": "~1.3.3",
    "gulp-cssimport": "~1.2.3",
    "gulp-coffeelint": "~0.4.0",
    "gulp-jsonlint": "~0.1.0",
    "gulp-concat": "~2.4.1",
    "gulp-notify": "~2.0.0",
    "gulp-grunt": "~0.5.2",
    "del": "~0.1.3",
    "run-sequence": "~1.0.1",
    "grunt-easymock": "~0.1.0",
    "browserify": "~6.2.0",
    "browserify-shim": "~3.8.0",
    "watchify": "~2.1.1",
    "coffeeify": "~0.7.0",
    "jadeify": "3.0.0",
    "vinyl-source-stream": "~1.0.0",
    "browser-sync": "~1.6.3",
    "proxy-middleware": "0.6.0",
    "http-proxy": "~1.6.1",
    "karma": "~0.12.16",
    "karma-mocha": "~0.1.4",
    "karma-chai": "~0.1.0",
    "karma-sinon": "~1.0.3",
    "karma-chrome-launcher": "~0.1.4",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-browserify": "~0.2.1",
    "karma-coffee-preprocessor": "~0.2.1",
    "require-dir": "~0.1.0",
    "coffee-script": "~1.6.3"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "jadeify"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "underscore": "./bower_components/underscore/underscore.js",
    "backbone": "./bower_components/backbone/backbone.js",
    "backbone-routefilter": "./bower_components/backbone-route-filter/backbone-route-filter.js",
    "backbone-stickit": "./bower_components/backbone.stickit/backbone.stickit.js",
    "backbone-validation": "./bower_components/backbone-validation/dist/backbone-validation.js",
    "marionette": "./bower_components/marionette/lib/backbone.marionette.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
    "jade": "./vendor/scripts/runtime.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "exports": "Backbone",
      "depends": ["jquery", "underscore"]
    },
    "backbone-routefilter": {
      "exports": "Backbone",
      "depends": ["backbone"]
    },
    "backbone-stickit": {
      "exports": "Backbone",
      "depends": ["backbone"]
    },
    "backbone-validation": {
      "exports": "Backbone",
      "depends": ["backbone"]
    },
    "marionette": {
      "exports": "Marionette",
      "depends": ["backbone"]
    },
    "bootstrap": {
      "depends": ["jquery"]
    },
    "templates": {
      "depends": ["jade"]
    }
  }
}
